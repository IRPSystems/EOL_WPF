<Window x:Class="EOL.Views.PasswordWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EOL.Views"
        mc:Ignorable="d"
        Title="Admin Password"
        Height="180" Width="300"
        WindowStartupLocation="CenterScreen">
    <StackPanel>
        <TextBlock Text="Enter Admin Password:" Margin="10"/>
        <PasswordBox Name="PasswordBox" Margin="10"/>
        <Button Content="OK" Width="75" Margin="10" Click="OKButton_Click"/>
    </StackPanel>
</Window>
